{
  "meta": {
    "name": "decentralized combat",
    "id": "combat_dc",
    "version": "v1.0.0",
    "repository": "https:\/\/github.com\/trendscenter\/combat-dc",
    "description": "Decentralized ComBat test"
  },
  "computation": {
    "type": "docker",
    "dockerImage": "coinstac/combat",
    "command": [
      "python",
      "\/computation\/local.py"
    ],
    "remote": {
      "type": "docker",
      "dockerImage": "coinstac/combat",
      "command": [
        "python",
        "\/computation\/remote.py"
      ]
    },
   "input": {
      "index": {
        "type": "number"
      },
      "url": {
        "type": "string"
      },

      "data": {
        "label": "responses",
        "type": "array",
        "items": ["responses"],
        "extensions": "csv"
      }

    },
    "output": {
      "sum": {
        "type": "number",
        "label": "Decentralized Sum"
      }
    },
    "display": {
      "type": "table"
    }
  }
}
